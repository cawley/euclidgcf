import java.util.Scanner;
class EuclideanAlgo{
  public static void main(String[] args) {
   
   System.out.println("Enter a positive number");
    int a = new Scanner(System.in).nextInt();
   System.out.println("Enter a positive number");
    int b = new Scanner(System.in).nextInt();
    
    for(int i = 0; i > 0; i++){
      for (int g = a % b; g >= 0; g++){
        System.out.println(g);
        
      }
      
    }

    
  }
}
